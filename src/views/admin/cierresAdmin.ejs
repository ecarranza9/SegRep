<%- include ('../partials/__header');  %> 


<div class="container-fluid pt-5">
    <div class="table-responsive">
        <table class="table table-hover" id="my_table">
        <thead>
        <tr>
        <th>N° Cierre</th>
        <th>Resolucion</th>
        <th>Tipo de avería</th>
        <th>Aprobación</th>
        <th>Observaciones</th>
        <th>Fecha de cierre</th>
        <th>Hora de cierre</th>
        <th>N° ticket correspondiente</th>
        </tr>
        </thead>

        <tbody>
            <tr>
                <%for(var i = 0; i < cierres.length; i++){%>

                    <td><a href="/admin/cierre/<%=cierres[i].id%>">Eliminar Cierre</a><%=cierres[i].id%></td>
                    <td><%=cierres[i].resolucion%></td>
                    <td><%=cierres[i].tipo_averia%></td>
                    <td><%=cierres[i].aprobacion%></td>
                    <td><%=cierres[i].observaciones%></td>
                    <td><%=cierres[i].fecha_cierre%></td>
                    <td><%=cierres[i].hora_cierre%></td>
                    <td><%=cierres[i].pedidoId%></td>

                <%}%>



            </tr>
        </tbody>

        </table>

        </div>
    </div>


    <%- include ('../partials/__footer');  %> 